{
  "redis": {
    "url": "redis://localhost:6379",
    "keyPrefix": "agimake:"
  },
  "indexer": {
    "include": [
      "**/*.ts",
      "**/*.tsx",
      "**/*.js",
      "**/*.jsx",
      "**/*.py",
      "**/*.go",
      "**/*.rs",
      "**/*.java",
      "**/*.c",
      "**/*.cpp",
      "**/*.h",
      "**/*.hpp",
      "**/*.md",
      "**/*.txt",
      "**/*.json",
      "**/*.yaml",
      "**/*.yml",
      "**/*.xml",
      "**/*.sh",
      "**/*.sql",
      "**/*.html",
      "**/*.css",
      "**/*.scss",
      "**/*.less",
      "**/*.vue",
      "**/*.svelte"
    ],
    "exclude": [
      "**/node_modules/**",
      "**/dist/**",
      "**/build/**",
      "**/.git/**",
      "**/vendor/**",
      "**/__pycache__/**",
      "**/coverage/**",
      "**/.next/**",
      "**/.nuxt/**",
      "**/.angular/**",
      "**/target/**",
      "**/cmake-build-*/**",
      "**/*.pt",
      "**/*.pth",
      "**/*.pkl",
      "**/*.pickle",
      "**/*.model",
      "**/*.bin",
      "**/*.onnx",
      "**/*.h5",
      "**/*.hdf5",
      "**/*.pb",
      "**/*.tflite",
      "**/*.torch",
      "**/*.ckpt",
      "**/*.pth.tar",
      "**/*.safetensors",
      "**/*.joblib",
      "**/*.npy",
      "**/*.npz",
      "**/*.parquet",
      "**/*.feather",
      "**/*.hdf",
      "**/*.h5",
      "**/*.msgpack",
      "**/*.arrow",
      "**/*.orc",
      "**/*.avro",
      "**/*.snappy",
      "**/*.lz4",
      "**/*.gzip",
      "**/*.gz",
      "**/*.zip",
      "**/*.tar",
      "**/*.tar.gz",
      "**/*.tgz",
      "**/*.rar",
      "**/*.7z",
      "**/*.bz2",
      "**/*.xz",
      "**/*.mp4",
      "**/*.avi",
      "**/*.mov",
      "**/*.mkv",
      "**/*.wmv",
      "**/*.flv",
      "**/*.webm",
      "**/*.m4v",
      "**/*.mp3",
      "**/*.wav",
      "**/*.flac",
      "**/*.aac",
      "**/*.ogg",
      "**/*.wma",
      "**/*.jpg",
      "**/*.jpeg",
      "**/*.png",
      "**/*.gif",
      "**/*.bmp",
      "**/*.tiff",
      "**/*.svg",
      "**/*.webp",
      "**/*.ico",
      "**/*.psd",
      "**/*.ai",
      "**/*.eps",
      "**/*.pdf",
      "**/*.doc",
      "**/*.docx",
      "**/*.xls",
      "**/*.xlsx",
      "**/*.ppt",
      "**/*.pptx",
      "**/*.csv",
      "**/*.tsv",
      "**/*.log",
      "**/*.out",
      "**/*.err",
      "**/*.dump",
      "**/*.sql",
      "**/*.db",
      "**/*.sqlite",
      "**/*.sqlite3",
      "**/*.har",
      "**/*.pcap",
      "**/*.trace"
    ],
    "maxFileSize": 262144
  },
  "pagerank": {
    "damping": 0.85,
    "iterations": 30,
    "tolerance": 1e-6
  },
  "watcher": {
    "enabled": true,
    "debounceMs": 1000
  },
  "git": {
    "enabled": true,
    "historyDepth": 100,
    "sampleWindowDays": 90,
    "gitBinary": "git"
  }
}
